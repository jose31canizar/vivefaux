{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "node_modules/.bin/nodemon index.js",
    "build-server": "babel -d ./build ./index.js",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js index.js --exec babel-node --presets es2015,stage-2",
    "rerun": "cd .. && rm -rf build && cd client && npm run build && cp -r build .. && cd .. && cd backend && npm run nodemon"
  },
  "proxy": "http://localhost:9001",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.3",
    "jade": "~1.11.0",
    "mongoose": "5.0.1",
    "node-gyp": "^3.8.0",
    "nodemailer": "^4.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.18.10",
    "randomatic": "^3.1.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
